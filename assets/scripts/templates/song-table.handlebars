{{#each songs as |song|}}

<tr data-id="{{song.id}}">
  <td class="table-instrument">{{song.instrument}}</td>
  <td>{{song.composer}}</td>
  <td>{{song.name}}</td>
  <td>
    <button class="cool-btn edit-song-btn">Edit</button>
    <button class="cool-btn delete-song-btn">Delete</button>
  </td>
</tr>

{{/each}}
