{{#each songs as |song|}}

<tr data-id="{{song.id}}">
  <td>{{song.instrument}}</td>
  <td>{{song.composer}}</td>
  <td>{{song.name}}</td>
  <td>
    <button class="btn btn-default edit-song-btn">Edit</button>
    <button class="btn btn-default delete-song-btn">Delete</button>
  </td>
</tr>

{{/each}}
